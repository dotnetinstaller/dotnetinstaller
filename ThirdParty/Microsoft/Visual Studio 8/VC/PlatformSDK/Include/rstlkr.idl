	DBLOCKMODE_NONE,
	DBLOCKMODE_READ,
	DBLOCKMODE_INTENT,
	DBLOCKMODE_WRITE
};

	HRESULT LockRowsByBookmark(
		[in] DBLOCKMODE eLockMode,
		[in] HCHAPTER hReserved,
		[in] DBCOUNTITEM cRows,
		[in, size_is((ULONG)cRows)] const DBBKMARK rgcbBookmarks[],
